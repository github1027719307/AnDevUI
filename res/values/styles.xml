<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light"></style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="top_category_scroll_view_item_text">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="left_drawer_item_line">
        <item name="android:background">@color/green</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1.0dip</item>
    </style>

    <style name="left_drawer_item_layout">
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/left_drawer_item_bg</item>
        <item name="android:paddingLeft">40.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">50.0dip</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="left_drawer_item_text">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">9.0dip</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:paddingBottom">9.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">25.0dip</item>
    </style>

    <style name="left_drawer_count" parent="@style/top_update_count">
        <item name="android:textSize">8.0sp</item>
        <item name="android:textColor">#ff000000</item>
    </style>

    <style name="top_update_count">
        <item name="android:textSize">10.0sp</item>
        <item name="android:textColor">#ffcc3131</item>
        <item name="android:gravity">center</item>
        <item name="android:height">15.0dip</item>
        <item name="android:width">15.0dip</item>
    </style>

    <style name="ActionSheet" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowAnimationStyle">@style/ActionSheetAnimation</item>
    </style>

    <style name="ActionSheetAnimation" parent="@android:style/Animation.Dialog">
        <item name="android:windowEnterAnimation">@anim/push_up_in</item>
        <item name="android:windowExitAnimation">@anim/push_up_out</item>
    </style>

    <style name="dot_style">
        <item name="android:layout_width">5dip</item>
        <item name="android:layout_height">5dip</item>
        <item name="android:background">@drawable/dot_normal</item>
        <item name="android:layout_marginLeft">1.5dip</item>
        <item name="android:layout_marginRight">1.5dip</item>
    </style>

    <style name="MyTextView"></style>

    <style name="SwipeBackLayout">
        <item name="edge_size">50dip</item>
        <item name="shadow_left">@drawable/shadow_left</item>
        <item name="shadow_right">@drawable/shadow_right</item>
        <item name="shadow_bottom">@drawable/shadow_bottom</item>
    </style>

    <style name="TransparentTheme" parent="@android:style/Theme.Holo.Light.NoActionBar">
        <item name="android:windowBackground">@null</item>
        <item name="android:actionBarStyle">@style/ActionBarStyle.Transparent</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="TransparentThemeWithActionBar" parent="@android:style/Theme.Holo.Light">
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="ActionBarStyle.Transparent" parent="@android:Widget.ActionBar">
        <item name="android:background">@null</item>
        <item name="android:displayOptions">homeAsUp|showHome|showTitle</item>
        <item name="android:titleTextStyle">@style/ActionBarStyle.Transparent.TitleTextStyle</item>
    </style>

    <style name="ActionBarStyle.Transparent.TitleTextStyle" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="item_txt">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/item_txt_color</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="item_right_txt">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:paddingRight">5.0dip</item>
    </style>

    <style name="login_account_txt">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/item_txt_color</item>
    </style>

    <style name="setting_item_head">
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/bg_row_head</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">42.0dip</item>
    </style>

    <style name="setting_checkbox">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">10.0dip</item>
        <item name="android:layout_weight">0.0</item>
    </style>

    <style name="setting_item_center" parent="@style/setting_item_head">
        <item name="android:background">@drawable/bg_row_center</item>
        <item name="android:layout_marginTop">-0.5dip</item>
    </style>

    <style name="setting_item_end" parent="@style/setting_item_head">
        <item name="android:background">@drawable/bg_row_end</item>
        <item name="android:layout_marginTop">-0.5dip</item>
    </style>

    <style name="setting_item_text">
        <item name="android:textSize">17.0sp</item>
        <item name="android:textColor">@color/setting_item_text</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10.0dip</item>
        <item name="android:layout_marginRight">10.0dip</item>
    </style>

    <style name="setting_copyright">
        <item name="android:textSize">13.0sp</item>
         <item name="android:padding">10.0dp</item>
        <item name="android:textColor">@color/setting_copyright</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10.0dip</item>
        <item name="android:text">@string/app_copyright</item>
    </style>

    <style name="setting_release_info">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">@color/setting_copyright</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">6.0dip</item>
        <item name="android:layout_marginBottom">8.0dip</item>
    </style>

    <style name="setting_alt_text">
        <item name="android:textSize">13.0sp</item>
        <item name="android:textColor">@color/setting_alt_text</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">5.0dip</item>
        <item name="android:layout_marginRight">5.0dip</item>
        <item name="android:layout_weight">0.0</item>
    </style>

    <style name="setting_hint_new">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">5.0dip</item>
        <item name="android:src">@drawable/icon_new</item>
        <item name="android:layout_weight">0.0</item>
    </style>

    <style name="setting_arrow" parent="@style/setting_checkbox">
        <item name="android:src">@drawable/arrow_drawer</item>
    </style>

    <style name="title_bar_style">
        <item name="android:background">@drawable/bg_titlebar</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">44.0dip</item>
    </style>

    <style name="back_view" parent="@style/title_bar_button">
        <item name="android:gravity">right|center</item>
        <item name="android:background">@drawable/transparent</item>
        <item name="android:paddingLeft">12.0dip</item>
        <item name="android:paddingRight">0.0dip</item>
        <item name="android:layout_marginLeft">0.0dip</item>
        <item name="android:layout_marginRight">0.0dip</item>
        <item name="android:minHeight">44.0dip</item>
        <item name="android:text">@null</item>
        <item name="android:drawableLeft">@drawable/btn_back</item>
        <item name="android:drawablePadding">0.0dip</item>
    </style>

    <style name="title_bar_button" parent="@style/common_button">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/btn_common_text</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@null</item>
        <item name="android:paddingLeft">17.0dip</item>
        <item name="android:paddingRight">17.0dip</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="common_button" parent="@style/text_on_button">
        <item name="android:background">@drawable/btn_common</item>
        <item name="android:paddingLeft">6.0dip</item>
        <item name="android:paddingRight">6.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minWidth">50.0dip</item>
        <item name="android:minHeight">30.0dip</item>
    </style>

    <style name="text_on_button">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:shadowRadius">0.0</item>
    </style>

    <style name="title_text_style">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textColor">@color/title_text_color</item>
    </style>

    <style name="subscribe_my_tip_text">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/subscribe_tip_text</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="subscribe_seperate_line">
        <item name="android:background">@color/subscribe_seperate_line</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.5dip</item>
        <item name="android:layout_marginTop">14.0dip</item>
        <item name="android:layout_marginBottom">14.0dip</item>
    </style>

    <style name="subscribe_more_tip_text" parent="@style/subscribe_my_tip_text">
        <item name="android:textSize">13.0sp</item>
    </style>

    <style name="line_style">
        <item name="android:layout_width">30dip</item>
        <item name="android:layout_height">4dip</item>
        <item name="android:background">@drawable/line_normal</item>
        <item name="android:layout_marginLeft">3dip</item>
        <item name="android:layout_marginRight">3dip</item>
    </style>

    <style name="line_style_black">
        <item name="android:layout_width">30dip</item>
        <item name="android:layout_height">4dip</item>
        <item name="android:background">@drawable/line_focused</item>
        <item name="android:layout_marginLeft">3dip</item>
        <item name="android:layout_marginRight">3dip</item>
    </style>

    <style name="PopMenuAnimation" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/slide_left_in</item>
        <item name="android:windowExitAnimation">@anim/slide_right_out</item>
    </style>

    <style name="Widget.GifMoviewView" parent="@android:style/Widget"></style>
        <!-- rank_tab_text -->
    <style name="app_text_style4">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@drawable/tab_text_selector</item>
    </style>
    <style name="search_tab_text">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@drawable/search_tab_text_selector</item>
    </style>
    <style name="splash_text">
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">@dimen/font_size_s</item>
        <item name="android:textColor">@color/white</item>
    </style>
</resources>